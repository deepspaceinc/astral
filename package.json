{
	"name": "astral",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "prettier --check . && xo",
		"lint:fix": "prettier --write . && xo --fix",
		"test": "jest",
		"start": "tsx src/cli.tsx"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"@pulumi/aws": "^6.79.1",
		"@pulumi/awsx": "^2.21.1",
		"@pulumi/pulumi": "^3.167.0",
		"@types/figlet": "^1.7.0",
		"@types/ink": "^2.0.3",
		"cli-table": "^0.3.11",
		"enquirer": "^2.4.1",
		"figlet": "^1.8.1",
		"find-process": "^1.4.10",
		"fullscreen-ink": "^0.0.2",
		"ink": "^4.4.1",
		"lucide-react": "^0.501.0",
		"pastel": "^2.0.0",
		"random-word-slugs": "^0.1.7",
		"react": "^18.2.0",
		"react-devtools-core": "^4.28.5",
		"terminal-link": "^4.0.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/jest": "^29.5.14",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"jest": "^29.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"tsx": "^4.19.4",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"bracketSpacing": true,
		"printWidth": 100,
		"semi": true,
		"endOfLine": "auto"
	}
}
